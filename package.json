{
  "name": "climate-data-viewer",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "rsbuild dev",
    "server:dev": "nodemon server.mjs",
    "build": "rsbuild build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "nodemon server.mjs",
    "test": "prettier --check . && eslint .",
    "clean": "prettier --write .",
    "serve": "node server.mjs"
  },
  "dependencies": {
    "@deck.gl/aggregation-layers": "^9.1.13",
    "@deck.gl/carto": "^9.1.13",
    "@deck.gl/core": "^9.1.13",
    "@deck.gl/layers": "^9.1.13",
    "@deck.gl/react": "^9.1.13",
    "@rsbuild/plugin-react": "^1.0.1-beta.14",
    "@rsbuild/plugin-sass": "^1.0.1-beta.14",
    "@tanstack/react-query": "^5.83.0",
    "babel-cli": "^6.26.0",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "deck.gl": "^9.1.13",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "immer": "^10.1.1",
    "lazily": "^2.0.5",
    "mapbox-gl": "^3.13.0",
    "maplibre-gl": "^5.6.1",
    "mjolnir.js": "^3.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-map-gl": "^8.0.4",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.8.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.0.1-beta.14",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.3"
  }
}
